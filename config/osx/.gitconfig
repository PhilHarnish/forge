[user]
	name = Phil Harnish
	email = philharnish@gmail.com
[github]
	user = PhilHarnish
[core]
  excludesfile = /Users/philharnish/.gitignore
[alias]
  b  = branch
  ci = commit
  co = checkout
  d  = diff -b
  dc = diff -b --cached
  ds = diff -b --stat
  s  = status
  # summary of what you're going to push
  ps = log --pretty=oneline origin..master
  # like "git log", but include the diffs
  w  = whatchanged -p
  # changes since we last did a push
  wo = whatchanged -p origin..
[color]
  ui = auto
